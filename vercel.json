{
  "version": 2,
  "builds": [
      {
          "src": "serve/dist/index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["server/dist/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "server/dist/index.js"
      }
  ]
}